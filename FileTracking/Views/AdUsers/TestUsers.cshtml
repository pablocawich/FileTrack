@model FileTracking.ViewModels.UserAdModel
@{
    ViewBag.Title = "TestUsers";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row">
    <div class="col-lg-12">
        <h2 class="page-header">User Test</h2>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <button class="btn btn-flickr" onclick="location.href = '@Url.Action("SaveAdUsers", "AdUsers")'" disabled>Pull From AD</button>
    <table class="table table-hover">
        <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Email</th>
            <th scope="col">Username</th>
            <th scope="col">Branch</th>
            <th scope="col">Role</th>
        </tr>
        </thead>
        <tbody>
        @foreach (var user in Model.UsersInRole)
        {
            <tr>
                <th scope="row">@user.Name</th>
                <td>@user.Email</td>
                <td>@user.Username</td>
                <td>@user.BranchesId</td>
                <td>@user.Role</td>
            </tr>
        }


        </tbody>
    </table>
</div>
