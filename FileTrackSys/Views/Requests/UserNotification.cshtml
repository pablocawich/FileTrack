@model IEnumerable<FileTracking.Models.Request>
@{
    ViewBag.Title = "UserNotification";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    <div class="col-lg-12">
        <h2 class="page-header">Notifications</h2>
    </div>
    <!-- /.col-lg-12 -->
</div>

<div class="row">
    <h4>The following has been accepted registry</h4>
    
    @foreach (var acceptedReq in Model)
    {
    <ul>
        <li>@acceptedReq.FileVolumes.FileNumber</li>
        <li>@acceptedReq.FileVolumes.Volume</li>
        <li>Accepted By: @acceptedReq.AcceptedBy</li>
    </ul>
        
    }
</div>
